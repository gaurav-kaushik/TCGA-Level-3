      "sbg:revision": 1
    }
  ],
  "sbg:image_url": null,
  "successCodes": [],
  "sbg:modifiedOn": 1459607345,
  "requirements": [
    {
      "requirements": [
        {
          "dockerPull": "rabix/js-engine",
          "class": "DockerRequirement"
        }
      ],
      "class": "ExpressionEngineRequirement",
      "id": "#cwl-js-engine"
    }
  ],
  "outputs": [
    {
      "id": "#output",
      "outputBinding": {
        "glob": "*.txt"
      },
      "type": [
        "null",
        "File"
      ]
    },
    {
      "id": "#BAM",
      "outputBinding": {
        "sbg:metadata": {
          "disease_type": {
            "class": "Expression",
            "script": "$job.inputs.Disease",
            "engine": "#cwl-js-engine"
          },
          "experimental_strategy": {
            "class": "Expression",
            "script": "$job.inputs.experimental_strat",
            "engine": "#cwl-js-engine"
          }
        },
        "glob": {
          "class": "Expression",
          "script": "{\n  dir = $job.inputs.UUID\n  return \"./\".concat(dir, \"/*.bam\")\n}",
          "engine": "#cwl-js-engine"
        }
      },
      "description": "bam file",
      "type": [
        "null",
        "File"
      ],
      "fileTypes": "BAM"
    },
    {
      "id": "#BAI",
      "outputBinding": {
        "sbg:metadata": {
          "disease_type": {
            "class": "Expression",
            "script": "$job.inputs.Disease",
            "engine": "#cwl-js-engine"
          },
          "experimental_strategy": {
            "class": "Expression",
            "script": "$job.inputs.experimental_strat",
            "engine": "#cwl-js-engine"
          }
        },
        "glob": {
          "class": "Expression",
          "script": "{\n  dir = $job.inputs.UUID \n  return \"./\".concat(dir, \"/*.bai\")\n}\n",
          "engine": "#cwl-js-engine"
        }
      },
      "label": "BAI file",
      "type": [
        "null",
        "File"
      ],
      "fileTypes": "BAI"
    }
  ],
