
{
  "label": "GeneTorrent",
  "sbg:modifiedBy": "gauravCGC",
  "sbg:sbgMaintained": false,
  "stdout": "stdout.txt",
  "arguments": [],
  "temporaryFailCodes": [],
  "sbg:id": "gauravCGC/devtools/genetorrent/1",
